<%if !admin_signed_in? %>	
	<% if user_signed_in? %>
		<li>
			<%= link_to('', edit_user_registration_path, method: :get, class: "glyphicon glyphicon-cog") %>
		</li>
	<% else %>
		<li>
			<%= button_to('Sign Up', new_user_registration_path, method: :get, class: "btn btn-default btn-sm navbar-btn")  %>
		</li>
	<% end %>
	
	<% if user_signed_in? && current_user.my_profile.blank? %>
		<li>			
			<%= button_to('New Profile', new_my_profile_path, method: :get, class: "btn btn-info btn-sm navbar-btn")%>
		</li>
	<% end %>

	<% if user_signed_in? && !current_user.my_profile.blank? %>
		<li>
			<%= button_to('My Profile', my_profile_path(current_user), method: :get, class: "btn btn-default btn-sm navbar-btn") %>
		</li>
	<% end %>
<% end %>